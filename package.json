{
    "name": "fetch-response-parser",
    "version": "0.0.3",
    "description": "Lightweight library to properly handle (JSON) responses from the Fetch API",
    "scripts": {
        "build:web": "cross-env TARGET=web npx webpack --mode production",
        "build:cjs": "npx webpack --mode production",
        "build": "run-s build:*",
        "dev:build:web": "cross-env TARGET=web npx webpack",
        "dev:build:cjs": "npx webpack --mode production",
        "dev:build": "run-s dev:build:*",
        "test": "jest",
        "clean": "rimraf dist fetch-response-parser-*.tgz"
    },
    "keywords": [
        "fetch",
        "response",
        "json"
    ],
    "author": "Skye Shaw <skye.shaw@gmail.com>",
    "license": "MIT",
    "homepage": "https://github.com/sshaw/fetch-response-parser",
    "repository": {
        "type": "git",
        "url": "https://github.com/sshaw/fetch-response-parser.git"
    },
    "bugs": {
        "url": "https://github.com/sshaw/fetch-response-parser/issues"
    },
    "browser": "dist/fetch-response-parser.js",
    "jsdelivr": "dist/fetch-response-parser.min.js",
    "devDependencies": {
        "@babel/cli": "^7.22.15",
        "babel-loader": "^9.1.3",
        "cross-env": "^7.0.3",
        "isomorphic-fetch": "^3.0.0",
        "jest": "30",
        "jest-fetch-mock": "^3.0.3",
        "npm-run-all": "^4.1.5",
        "rimraf": "^5.0.5",
        "webpack": "^5.88.2",
        "webpack-cli": "^5.1.4"
    }
}
